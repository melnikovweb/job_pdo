{
  "plugins": [
    "@babel/syntax-dynamic-import",
    [
      "module-resolver",
      {
        "root": [
          "./"
        ],
        "alias": {
          "@shared": "./src/shared/",
          "underscore": "lodash"
        }
      }
    ]
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ]
  ]
}
